{
  "name": "@jackdomleo7/vue3-library",
  "version": "0.0.0",
  "description": "A Vue3 component library for my own enjoyment.",
  "repository": {
    "type": "git",
    "directory": "git+https://github.com/jackdomleo7/Vue3_Component_Library.git"
  },
  "author": {
    "name": "Jack Domleo",
    "url": "https://jackdomleo.dev",
    "email": "jackdomleo7@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/jackdomleo7/Vue3_Component_Library/issues"
  },
  "main": "dist/jackdomleo7.ssr.js",
  "browser": "dist/jackdomleo7-vue3-library.esm.js",
  "module": "dist/jackdomleo7-vue3-library.esm.js",
  "unpkg": "dist/jackdomleo7-vue3-library.min.js",
  "types": "dist/types/src/entry.esm.d.ts",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config rollup.config.js",
    "lint": "vue-cli-service lint",
    "postbuild": "rimraf ./dist/types/dev ./dist/types/src/entry.d.ts",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@rollup/plugin-alias": "^3.1.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "@vue/compiler-sfc": "^3.2.23",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "lint-staged": "^9.5.0",
    "minimist": "^1.2.5",
    "postcss": "^8.4.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.60.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.44.0",
    "sass-loader": "^10.2.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.5.2",
    "vue": "^3.2.23"
  },
  "peerDependencies": {
    "vue": "3.x"
  },
  "engines": {
    "node": ">=16",
    "npm": "8"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
