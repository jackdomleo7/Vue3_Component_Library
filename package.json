{
  "name": "@jackdomleo7/vue3-library",
  "version": "0.0.0",
  "description": "A Vue3 component library for my own enjoyment.",
  "repository": {
    "type": "git",
    "directory": "git+https://github.com/jackdomleo7/Vue3_Component_Library.git"
  },
  "author": {
    "name": "Jack Domleo",
    "url": "https://jackdomleo.dev",
    "email": "jackdomleo7@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/jackdomleo7/Vue3_Component_Library/issues"
  },
  "main": "dist/jackdomleo7.ssr.js",
  "browser": "dist/jackdomleo7-vue3-library.esm.js",
  "module": "dist/jackdomleo7-vue3-library.esm.js",
  "unpkg": "dist/jackdomleo7-vue3-library.min.js",
  "types": "dist/types/src/entry.esm.d.ts",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "src/**/!(*.spec).ts"
  ],
  "sideEffects": false,
  "scripts": {
    "prepack": "npm run build",
    "serve": "vue-cli-service serve dev/serve.ts",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config rollup.config.js",
    "postbuild": "rimraf ./dist/types/dev ./dist/types/src/entry.d.ts",
    "lint:check": "eslint --ext .ts,.js,.vue --ignore-path .gitignore --max-warnings=0 .",
    "lint:fix": "eslint --ext .ts,.js,.vue --ignore-path .gitignore --max-warnings=0 --cache --fix .",
    "test": "jest",
    "test:update-snapshots": "jest --updateSnapshot",
    "test:report": "open-cli report/index.html"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@rollup/plugin-alias": "^3.1.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/jest": "^27.0.3",
    "@types/jest-axe": "^3.5.3",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-plugin-typescript": "^4.5.15",
    "@vue/cli-plugin-unit-jest": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.0-rc.17",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "babel-jest": "^27.4.5",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "jest": "^27.4.5",
    "jest-axe": "^5.0.1",
    "jest-stare": "^2.3.0",
    "lint-staged": "^12.1.2",
    "minimist": "^1.2.5",
    "open-cli": "^7.0.1",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.61.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.45.0",
    "sass-loader": "^10.2.0",
    "ts-jest": "^27.1.2",
    "ttypescript": "^1.5.13",
    "typescript": "^4.5.4",
    "vue": "^3.2.26"
  },
  "peerDependencies": {
    "vue": "3.x"
  },
  "engines": {
    "node": ">=16",
    "npm": "8"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "npm run lint:fix",
      "git add"
    ]
  }
}
