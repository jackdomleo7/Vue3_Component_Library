{
  "name": "@jackdomleo7/vue3-library",
  "version": "0.0.0",
  "description": "A Vue3 component library for my own enjoyment.",
  "repository": {
    "type": "git",
    "directory": "git+https://github.com/jackdomleo7/Vue3_Component_Library.git"
  },
  "author": {
    "name": "Jack Domleo",
    "url": "https://jackdomleo.dev",
    "email": "jackdomleo7@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/jackdomleo7/Vue3_Component_Library/issues"
  },
  "main": "dist/jackdomleo7.ssr.js",
  "browser": "dist/jackdomleo7-vue3-library.esm.js",
  "module": "dist/jackdomleo7-vue3-library.esm.js",
  "unpkg": "dist/jackdomleo7-vue3-library.min.js",
  "types": "dist/types/src/entry.esm.d.ts",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "src/**/!(*.spec|*.stories).ts"
  ],
  "scripts": {
    "prepack": "npm run build",
    "serve": "start-storybook -p 6006",
    "serve:static": "npm run build:docs && http-server docs --port 6007 -a localhost",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config rollup.config.js",
    "postbuild": "rimraf ./dist/types/dev ./dist/types/src/entry.d.ts",
    "build:docs": "rimraf ./docs && build-storybook --output-dir ./docs",
    "lint:check": "eslint --ext .ts,.js,.vue --ignore-path .gitignore --max-warnings=0 .",
    "lint:fix": "eslint --ext .ts,.js,.vue --ignore-path .gitignore --max-warnings=0 --cache --fix .",
    "test": "jest",
    "test:update-snapshots": "jest --updateSnapshot",
    "test:report": "open-cli report/index.html"
  },
  "devDependencies": {
    "@babel/core": "^7.16.10",
    "@babel/preset-env": "^7.16.10",
    "@babel/preset-typescript": "^7.16.7",
    "@hover/storybook-addon-pseudo-states": "^1.1.3",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.0.1",
    "@storybook/addon-a11y": "^6.4.13",
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-essentials": "^6.4.13",
    "@storybook/addon-links": "^6.4.13",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/vue3": "^6.4.13",
    "@types/jest": "^27.4.0",
    "@types/jest-axe": "^3.5.3",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-plugin-typescript": "^4.5.15",
    "@vue/cli-plugin-unit-jest": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "@vue/compiler-sfc": "^3.2.27",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-storybook": "^0.5.6",
    "eslint-plugin-vue": "^8.3.0",
    "http-server": "^14.1.0",
    "jest": "^27.4.7",
    "jest-axe": "^5.0.1",
    "jest-stare": "^2.3.0",
    "minimist": "^1.2.5",
    "open-cli": "^7.0.1",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "push-dir": "^0.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.64.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.49.0",
    "sass-loader": "^10.2.1",
    "ts-jest": "^27.1.3",
    "ttypescript": "^1.5.13",
    "typescript": "^4.5.4",
    "vue": "^3.2.27",
    "vue-loader": "^16.8.3"
  },
  "peerDependencies": {
    "vue": "3.x"
  },
  "engines": {
    "node": ">=16",
    "npm": "8"
  }
}
